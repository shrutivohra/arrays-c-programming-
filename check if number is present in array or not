#include<stdio.h>
int main()
{
	int a[100],i,n,key,count=0;//n is number of elements in array,i is count variable,key is number to be searched
	printf("enter number of elements in array");
	scanf("%d",&n);
	printf("enter elements of array");
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	printf("enter the key");
	scanf("%d",&key);
	//check if the key is present in array or not
	for(i=0;i<n;i++)
	{
		if(a[i]==key)
		{
		
		printf("%d is present in array",key);
		count++;
		break;
	}
		
	}
	if(count==0)
	printf("%d is not present in array",key);
	return 0;
}
